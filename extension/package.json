{
  "name": "patient-data-extraction-extension",
  "displayName": "Patient Data Extractor",
  "version": "1.0.0",
  "description": "Chrome extension for extracting patient information from Google Forms",
  "scripts": {
    "dev": "plasmo dev --verbose",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.15.0",
    "axios": "^1.11.0",
    "lucide-react": "^0.525.0",
    "plasmo": "^0.90.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@types/chrome": "^0.0.258",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.3"
  },
  "manifest": {
    "permissions": [
      "activeTab",
      "storage",
      "scripting",
      "tabs"
    ],
    "host_permissions": [
      "http://localhost:3000/*"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/*"
        ],
        "matches": [
          "https://docs.google.com/*"
        ]
      }
    ],
    "action": {}
  }
}
